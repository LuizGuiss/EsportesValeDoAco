quadra) => (\r\n          <Marker\r\n            key={quadra.id}\r\n            position={[quadra.latitude, quadra.longitude]}\r\n            icon={mapIcon}\r\n          >\r\n            <Popup\r\n              closeButton={false}\r\n              minWidth={240}\r\n              maxWidth={240}\r\n              className=\"map-popup\"\r\n            >\r\n              {quadra.name}\r\n              <Link to={`/quadras/${quadra.id}`}>\r\n                <FiArrowRight size={20} color=\"#fff\" />\r\n              </Link>\r\n            </Popup>\r\n\r\n          </Marker>\r\n        ))}\r\n\r\n      </MapContainer>\r\n\r\n      <Link to=\"/quadras/create\" title=\"Cadastrar quadra\" className=\"create-quadra\">\r\n        <FiPlus size={32} color=\"#fff\" />\r\n      </Link>\r\n\r\n      {authenticated &&\r\n        <Link to=\"/dashboard/quadras-registered\" className=\"dashboard\">\r\n          Dashboard\r\n        </Link>\r\n      }\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default QuadrasMap;",["125","126"],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesValeDoAco\\web\\src\\App.tsx",[],["127","128"],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesValeDoAco\\web\\src\\pages\\Landing.tsx",["129"],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesValeDoAco\\web\\src\\utils\\mapIcon.ts",[],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesValeDoAco\\web\\src\\routes.tsx",[],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesValeDoAco\\web\\src\\services\\api.ts",[],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesValeDoAco\\web\\src\\index.tsx",[],["130","131"],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesValeDoAco\\web\\src\\redux\\types.ts",[],"C:\\Users\\luizg\\Desktop\\Facul\\Segundo semestre 2020\\LES\\meuTrabalho\\EsportesVal